{
	"compilerOptions": {
		"resolveJsonModule": true,
		"module": "commonjs",
		"esModuleInterop": true,
		"target": "es6",
		"moduleResolution": "node",
		"sourceMap": true,
		"outDir": "dist",
		"paths": {
			"@src/*": ["./src/*"],
			"@Shared/*": ["./Shared/*"]
		}
	},
	"resolveJsonModule": true,
	"allowSyntheticDefaultImports": true,
	// "include": ["*.ts"],
	"exclude": ["node_modules"]
}
